st = None\n","    self.y_train = None\n","    self.y_test = None\n","\n","  def importar_dados(self, url):\n","    !wget {url} -O dataset_avaliacoes_classe.csv\n","    self.df = pd.read_csv('dataset_avaliacoes_classe.csv')\n","    if self.df is not None:\n","      print('='*53)\n","      print('Dados importados com sucesso!')\n","    else:\n","      print('Erro ao importar dados!')\n","    return self.df\n","\n","  def vetorizar_dados(self, dataframe):\n","    df = dataframe\n","    self.bag_of_words = self.vectorizer.fit_transform(df.avaliacao)\n","\n","    if self.bag_of_words is not None:\n","      print('Dados vetorizados com sucesso!')\n","    else:\n","      print('Erro ao vetorizar dados!')\n","    return self.bag_of_words\n","\n","  def separando_dados(self, words, dataframe):\n","    bag_of_words = words\n","    df = dataframe\n","    self.x_train, self.x_test, self.y_train, self.y_test = train_test_split(bag_of_words, df.sentimento, random_state=42)\n","    return self.x_train, self.x_test, self.y_train, self.y_test\n","\n","  def treinar_modelo(self, x_train, y_train):\n","    self.regressor = LogisticRegression()\n","    self.regressor.fit(x_train, y_train)\n","\n","    if self.regressor is not None:\n","      print('Modelo treinado com sucesso!')\n","    else:\n","      print('Erro ao treinar modelo!')\n","    return self.regressor"],"metadata":{"id":"jKh1lPJqCZQj","executionInfo":{"status":"ok","timestamp":1769478919905,"user_tz":180,"elapsed":1,"user":{"displayName":"Victor Hugo do Nascimento","userId":"04558136456232909731"}}},"execution_count":88,"outputs":[]},{"cell_type":"code","source":["url = 'https://raw.githubusercontent.com/alura-cursos/nlp_analise_sentimento/refs/heads/main/Dados/dataset_avaliacoes.csv'\n","\n","analise = Analise_Sentimento()\n","df_analise = analise.importar_dados(url)\n","bag_of_words_analise = analise.vetorizar_dados(df_analise)\n","x_train, x_test, y_train, y_test = analise.separando_dados(bag_of_words_analise, df_analis