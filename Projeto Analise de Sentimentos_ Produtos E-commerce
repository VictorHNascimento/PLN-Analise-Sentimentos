mport tokenize\n","from nltk import ngrams\n","from sklearn.feature_extraction.text import TfidfVectorizer\n","\n","import seaborn as sns\n","import unidecode\n","import pandas as pd"],"metadata":{"id":"jwgxRtADEeVA","executionInfo":{"status":"ok","timestamp":1769790172963,"user_tz":180,"elapsed":5,"user":{"displayName":"Victor Hugo do Nascimento","userId":"04558136456232909731"}}},"execution_count":70,"outputs":[]},{"cell_type":"markdown","source":["## Classe Analise_Sentimento"],"metadata":{"id":"rpTf3djDCS77"}},{"cell_type":"code","source":["class Analise_Sentimento():\n","\n","  def __init__(self):\n","    self.df = None\n","    self.df_processado = None\n","    self.tfidf = TfidfVectorizer(lowercase=False, max_features=1000, ngram_range=(1,2))\n","    self.vectorizer = None\n","    self.regressor = None\n","    self.x_train = None\n","    self.x_test = None\n","    self.y_train = None\n","    self.y_test = None\n","    self.tokens = None\n","    self.frequencia = None\n","\n","  def importar_dados(self, url):\n","    !wget {url} -O dataset_avaliacoes_classe.csv\n","    self.df = pd.read_csv('dataset_avaliacoes_classe.csv')\n","    if self.df is not None:\n","      print('='*53)\n","      print('Dados importados com sucesso!')\n","    else:\n","      print('Erro ao importar dados!')\n","    return self.df\n","\n","  def vetorizar_dados(self, dataframe):\n","    df = dataframe\n","    self.tfidf = TfidfVectorizer(lowercase=False, max_features=1000, ngram_range=(1,2))\n","    self.vectorizer = self.tfidf.fit_transform(df['avaliacao_processada'])\n","\n","    if  self.tfidf is not None:\n","      print('Dados vetorizados com sucesso!')\n","    else:\n","      print('Erro ao vetorizar dados!')\n","    return  self.tfidf, self.vectorizer\n","\n","  def separando_dados(self, dataframe, vetor_tfidf):\n","    df = dataframe\n","    self.x_train, self.x_test, self.y_train, self.y_test = train_test_split(vetor_tfidf, df['sentimento'], random_state=4978)\n","    return self.x_train, self.x_test, self.y_train, self.y_test\n","\n","  def treinar_modelo(self, x_train, y_train):\n","    self.regressor = LogisticRegression()\n","    self.regressor.fit(x_train, y_train)\n","\n","    if self.regressor is not None:\n","      print('Modelo treinado com sucesso!')\n","    else:\n","      print('Erro ao treinar modelo!')\n","    return self.regressor\n","\n","  def limpeza_texto(self, dataframe):\n","    df = dataframe\n","    stemmer = nltk.RSLPStemmer()\n","    tokenizer = tokenize.WordPunctTokenizer()\n","    stopwords = [unidecode.unidecode(texto) for texto in nltk.corpus.stopwords.words('portuguese')]\n","    frases_processadas = []\n","\n","    for avaliacao in df['avaliacao']:\n","      palavras_pontuacao = tokenizer.tokenize(avaliacao)\n","\n","      frase_pontuacao = [palavra for palavra in palavras_pontuacao if palavra.isalpha()]\n","      frase_acentuacao = [unidecode.unidecode(palavra) for palavra in frase_pontuacao]\n","      frase_minuscula = [palavra.lower() for palavra in frase_acentuacao]\n","      frase_stopwords = [palavra for palavra in frase_minuscula if palavra not in stopwords]\n","      nova_frase = [stemmer.stem(palavra) for palavra in frase_stopwords]\n","\n","      frases_processadas.append(' '.join(nova_frase))\n","\n","    df_temp = pd.DataFrame(frases_processadas, columns=['avaliacao'])\n","    posicao = df.columns.get_loc('avaliacao') + 1\n","    df.insert(posicao, 'avaliacao_processada', df_temp['avaliacao'])\n","    self.df_processado = df\n","    return self.df_processado\n","\n","  def tokentizacao(self, dataframe):\n","    df = dataframe\n","    tokenizer = tokenize.WhitespaceTokenizer()\n","    palavras = ' '.join([texto for texto in df.avaliacao_processada])\n","    self.tokens = tokenizer.tokenize(palavras)\n","    return self.tokens\n","\n","\n","  def calcula_frequencia(self, tokens):\n","    self.frequencia = nltk.FreqDist(tokens)\n","    return self.frequencia\n","\n","  def grafico_frequencia(self, frequencia, quantidade):\n","    df_frequencia = pd.DataFrame({\"Palavra\": list(frequencia.keys()),\n","                                  \"Frequência\": list(frequencia.values())})\n","\n","    df_frequencia = df_frequencia.nlargest(columns=\"Frequência\", n=quantidade)\n","    plt.figure(figsize=(20,6))\n","    ax = sns.barplot(data=df_frequencia, x=\"Palavra\", y=\"Frequência\", color='gray')\n","    ax.set(ylabel=\"Contagem\")\n","    plt.show()\n"],"metadata":{"id":"jKh1lPJqCZQj","executionInfo":{"status":"ok","timestamp":1769790172968,"user_tz":180,"elapsed":3,"user":{"displayName":"Victor Hugo do Nascimento","userId":"04558136456232909731"}}},"execution_count":71,"outputs":[]},{"cell_type":"markdown","source":["## Criando o Modelo"],"metadata":{"id":"8RcF5QAFIib9"}},{"cell_type":"markdown","source":["###Importando os Dados"],"metadata":{"id":"Kq01BcTphmUA"}},{"cell_type":"code","source":["url = 'https://raw.githubusercontent.com/alura-cursos/nlp_analise_sentimento/refs/heads/main/Dados/dataset_avaliacoes.csv'\n","modelagem = Analise_Sentimento()\n","df_modelagem = modelagem.importar_dados(url)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"8X6VpkqZDcTK","executionInfo":{"status":"ok","timestamp":1769790173368,"user_tz":180,"elapsed":397,"user":{"displayName":"Victor Hugo do Nascimento","userId":"04558136456232909731"}},"outputId":"3c82c6f1-7e40-409d-b6cd-76bd156f8d86"},"execution_count":72,"outputs":[{"output_type":"stream","name":"stdout","text":["--2026-01-30 16:22:51--  https://raw.githubusercontent.com/alura-cursos/nlp_analise_sentimento/refs/heads/main/Dados/dataset_avaliacoes.csv\n","Resolving raw.githubusercontent.com (raw.githubusercontent.com)... 185.199.110.133, 185.199.109.133, 185.199.108.133, ...\n","Connecting to raw.githubusercontent.com (raw.githubusercontent.com)|185.199.110.133|:443... connected.\n","HTTP request sent, awaiting response... 200 OK\n","Length: 2558643 (2.4M) [text/plain]\n","Saving to: ‘dataset_avaliacoes_cl